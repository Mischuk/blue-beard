/*!=================
  Base layout
====================*/
html, body
  height 100%
  min-width $min-width + 50
  min-height 600px
.container
  min-height 100%
  min-width $min-width
  margin-bottom -($footer-height)
  &:after
    content ''
    display block
    height $footer-height + $footer-margin-top
footer, .container:after
  height $footer-height
  min-width: $min-width
.wrapper
  width $min-width
  position relative
  margin 0 auto
  height 100%
/*!=================
  Basic elements
====================*/
img{
  display: inline-block;
  vertical-align: middle;
  max-width: 100%;
  max-height: 100%;
  width: auto;
  height: auto;
}
.inp-textarea,
textarea{
  max-height: 400px;
  max-width: 100%;
}


.main
  width 100%
  height 100%
  position relative
  background url('../images/bg.jpg') no-repeat 50% 50%
  background-size cover
  overflow hidden
  &:after
    content ''
    width 2040px
    height 950px
    position absolute
    left 50%
    top 50%
    margin-left -(@width/2)
    margin-top -(@height/2)
    background url('../images/aorte-ready.png') no-repeat 0 0
    z-index 10
.logo
  position absolute
  left 50%
  top 50%
  width 294px
  height 352px
  margin-left -(@width/2)
  margin-top -(@height/2)
  z-index 100
  background url('../images/logo-bg.png') no-repeat 0 0
  h1
    position absolute
    left 5px
    bottom 20px
    text-align center
    width 100%
    font-family 'Lobster', cursive
    color #fff
    text-shadow 0 1px 16px rgba(0,0,0,.75)
    font-size 40px
    line-height 1
  a
    display block
    width 100%
    height 100%
    &:before
      content ''
      width 148px
      height 240px
      background url('../images/head.png') no-repeat 0 0
      position absolute
      left 50%
      top 22px
      margin-left -(@width/2)
    &:after
      content ''
      width 252px
      height 357px
      background url('../images/logo-top.png') no-repeat 0 0
      position absolute
      left 50%
      top 0
      margin-left -(@width/2)
@keyframes animated
  0%
    transform scale(1) translateY(0)
    animation-timing-function ease
  50%
    transform scale(1.03) translateY(-2px)
    animation-timing-function ease
  100%
    transform scale(1) translateY(0)
    animation-timing-function ease

.logo a:before
  animation-name animated
  animation-duration 1.5s
  animation-timing-function ease
  animation-iteration-count infinite
  animation-direction normal

.loaded .title
  top -10px
  left 50%
.title
  width 294px
  height 156px
  background url('../images/title.png') no-repeat 0 0
  position absolute
  left 51%
  margin-left -(@width/2)
  top -90px
  text-align center
  animation-name titled
  animation-duration 1s
  animation-timing-function ease-in
  animation-iteration-count 4
  animation-direction normal
  anim(all, .9s)
  z-index 100
  cursor default
  span
    font-size 35px
    color #fff
    text-shadow 1px 0 1px rgba(0,0,0,.85)
    font-family 'Lobster', cursive
    display inline-block
    vertical-align middle
    margin-top 100px

@keyframes titled
  0%
    transform rotate(0deg)
  25%
    transform rotate(3deg)
  50%
    transform rotate(0deg)
  75%
    transform rotate(-1deg)
  100%
    transform rotate(0deg)

.menu
  position absolute
  left 50%
  top 50%
  width 1020px
  height 530px
  margin-left -(@width/2)
  margin-top -(@height/2)
.menu-item
  position absolute
  width 190px
  height 62px
  left 50%
  top 50%
  margin-left -(@width/2)
  margin-top -(@height/2)
  a
    display block
    width 100%
    height 100%
    &:hover span
      color yellow
    span
      display block
      position absolute
      left 0
      bottom 0px
      line-height 62px
      text-align center
      color #fff
      font-family 'Lobster', cursive
      font-size 30px
      text-shadow 1px 0 1px rgba(0,0,0,.84)
      anim(all, .35s)
      background url('../images/table.png') no-repeat 0 0
      width 100%
      height 100%
      z-index 11

